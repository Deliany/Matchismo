<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>--company-id</key>
        <string>com.clearsky</string> <!-- YourCompanyID -->
        <key>--create-docset</key>
        <true/>
        <key>--ignore</key>
        <array>
            <string>ThirdParty</string>
            <string>Libraries</string>
            <string>Frameworks</string>
            <string>Testing</string>
            <string>ViewController.h</string>
            <string>AppDelegate.h</string>
            <string>.m</string>
        </array>
        <!-- <key>--index-desc</key>
        <string>${PROJECT_DIR}/readme.markdown<string/> -->
        <key>--install-docset</key>
        <true/>
        <key>--docset-platform-family</key>
        <string>iphoneos</string>
        <key>--output</key>
        <string>Docs/</string> <!-- Output -->
        <key>--project-company</key>
        <string>Deliany Production</string> <!-- YourProjectName -->
        <key>--project-name</key>
        <string>iMatch</string> <!-- YourProjectName -->
        <key>--project-version</key>
        <string>1.0</string> <!-- YourDocumentationVersion  -->
        <key>--keep-undocumented-objects</key>
        <true/>
        <key>--keep-undocumented-members</key>
        <true/>
        <key>--warn-undocumented-object</key>
        <true/>
        <key>--warn-undocumented-member</key>
        <true/>
        <key>--warn-empty-description</key>
        <true/>
        <key>--warn-missing-arg</key>
        <true/>
        <key>--warn-unknown-directive</key>
        <true/>
        <key>--keep-intermediate-files</key>
        <true/>
        <key>--explicit-crossref</key>
        <true/>
        <key>--no-repeat-first-par</key>
        <true/>
        <key>--logformat</key>
        <string>xcode</string>
        <key>--exit-threshold</key>
        <integer>2</integer>        
    </dict>
</plist>
